<%- model_class = Squeek -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>

<%= render :partial => 'form' %>

<div class="content">
  <% @squeeks.each do |squeek| %>
    <div class="well">
      <p><strong><%= squeek.user %></strong> (<%= distance_of_time_in_words(Time.now, squeek.created_at) %> ago).</p>
      <p><%= squeek.message %></p> 
    </div>
  <% end %>
</div>
